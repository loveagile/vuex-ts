{
  "root": true,
  "extends": "@1stg",
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.vue"
      ],
      "rules": {
        "vue/no-multiple-template-root": 0,
        "vue/require-default-prop": 0
      }
    },
    {
      "files": [
        "*.ts"
      ],
      "rules": {
        "@typescript-eslint/consistent-type-imports": 2,
        "import/order": [
          2,
          {
            "alphabetize": {
              "order": "asc"
            },
            "newlines-between": "always"
          }
        ]
      }
    }
  ]
}
