{
  "root": true,
  "extends": "@1stg",
  "rules": {
    "unicorn/prefer-export-from": [
      2,
      {
        "ignoreUsedVariables": true
      }
    ]
  },
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.vue"
      ],
      "rules": {
        "vue/require-default-prop": 0
      }
    },
    {
      "files": [
        "*.ts"
      ],
      "rules": {
        "@typescript-eslint/consistent-type-imports": 2
      }
    }
  ]
}
