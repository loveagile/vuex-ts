{
  "name": "@rxts/vue",
  "version": 2,
  "alias": [
    "rx-vue.now.sh",
    "vue-rx.now.sh"
  ],
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/vue-(?<id>[^/]*)",
      "dest": "/vue-$id/docs/index.html"
    },
    {
      "src": "/*",
      "status": 301,
      "headers": {
        "Location": "/vue-translator"
      }
    }
  ]
}
